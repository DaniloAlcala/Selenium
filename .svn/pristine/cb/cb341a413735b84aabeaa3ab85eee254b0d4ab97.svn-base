								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setObservaciones(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Actuaciones.toString().trim())) Actuaciones.setObservaciones(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Archivo.toString().trim())) Archivo.setObservaciones(valor);
								break;
							case "FechaInicio":
								Cargo.setFechaInicio(valor);
								break;
							case "SegundoCUIJ":
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setSegundoCUIJ(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Actuaciones.toString().trim())) Actuaciones.setSegundoCUIJ(valor);
								break;
							case "Matricula":
								Expediente.setMatricula(valor);
								break;
							case "Tipo":
								Expediente.setTipo(valor);
								break;
							case "EnviarA":
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setEnviarA(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setEnviarA(valor);
								break;
							case "Motivo":
