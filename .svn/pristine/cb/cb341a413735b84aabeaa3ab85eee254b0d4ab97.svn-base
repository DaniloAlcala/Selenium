		public static void CargaExcelResultados() {
			try {
				FileInputStream file = new FileInputStream(new File(getExcelDatos()));
				eExcelDatos = new XSSFWorkbook(file);
				file.close();

			} catch (Exception e) {
				
				e.printStackTrace();
			}
		}
		
		public static void CerrarExcelResultados() {
			try {
				eExcelDatos.close();
			} catch (IOException e) {
		
				e.printStackTrace();
			}
		}
		
		public static void CargarDatosTestlink() {
			
			XSSFSheet sheet = eExcelDatos.getSheetAt(0);
			
			Iterator<Row> rowIterator = sheet.iterator();
			Row row = rowIterator.next(); 
			
			row = rowIterator.next(); 
			Cell cellTestProject = row.getCell(6);
			setTestProject(cellTestProject.getStringCellValue());
			
			row = rowIterator.next();
			Cell cellTestPlan = row.getCell(6);
			setTestPlan(cellTestPlan.getStringCellValue());
			
			row = rowIterator.next(); 
			Cell cellBuild = row.getCell(6);
			setBuild(cellBuild.getStringCellValue());
							
			CerrarExcelResultados();
		}

		public static void CargarParametrosDeEjecucion() {

			InputStream inputStream=null;

			try {

				prop = new Properties();
				String propFileName = "EjecucionPruebas.properties";

				inputStream = GestorDeDatosDePrueba.class.getClassLoader().getResourceAsStream(propFileName);
				
				if (inputStream != null) {
					prop.load(inputStream);
					ExcelDatos = prop.getProperty("ExcelDatos");
					Usuario = prop.getProperty("usuario");
					Contrasena = prop.getProperty("password");
					Entorno= prop.getProperty("entorno");
					Juzgado = prop.getProperty("juzgadoPerfil");
					DevKey = prop.getProperty("devKey");
					UrlTestlink = prop.getProperty("urlTestlink");
					IconoExpediente = prop.getProperty("iconoExpediente");
					JuzgadoMEU = prop.getProperty("MEU");
					JuzgadoMenores= prop.getProperty("juzgadoMenores");
					AG_Usuario = prop.getProperty("AG_usuario");
					AG_Contrasena = prop.getProperty("AG_password");
					AG_Entorno= prop.getProperty("AG_entorno");
					AG_Juzgado = prop.getProperty("AG_juzgadoPerfil");
					MESA_Usuario = prop.getProperty("MESA_usuario");
					MESA_Contrasena = prop.getProperty("MESA_password");
					MESA_Entorno= prop.getProperty("MESA_entorno");
					MESA_Juzgado = prop.getProperty("MESA_juzgadoPerfil");
					MESA_iconoExpediente = prop.getProperty("MESA_iconoExpediente");
					// Desde aqui intento generar codigo para abrir varios entornos en la misma prueba con un mismo Driver
					UsuarioInst2 = prop.getProperty("usuarioInst2");
					ContrasenaInst2 = prop.getProperty("passwordInst2");
					EntornoInst2= prop.getProperty("entornoInst2");
					IconoExpedienteInst2 = prop.getProperty("iconoExpedienteInst2");
					
					inputStream.close();
					
				} else {
					
					throw new FileNotFoundException("property file '" + propFileName + "' not found in the classpath");
				}

			} catch (Exception e) {
				System.out.println("Exception: " + e);
			} 
			
		}
		
