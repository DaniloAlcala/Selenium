								break;
							case "ApellidoYNombre":
								Cargo.setActor(valor);
								break;
							case "Domicilio":
								Cargo.setDomicilio(valor);
								break;
							case "Ubicacion":
								Cargo.setUbicacion(valor);
								break;
							case "Observaciones":
