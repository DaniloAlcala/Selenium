								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setModelo(valor);
								break;
							case "Fecha":
								if (nombreEntidad.toString().trim().equals(entidades.MEU.toString().trim())) MEU.setFecha(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Menores.toString().trim())) Menores.setFecha(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setFecha(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setFecha(valor);
								break;
							case "Juzgado":
								MEU.setJuzgado(valor);
								if (nombreEntidad.toString().trim().equals(entidades.MEU.toString().trim())) MEU.setJuzgado(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setJuzgado(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setJuzgado(valor);
