								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setSolicita(valor);;
								break;
							case "GrupoOClase":
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setGrupoClase(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setGrupoClase(valor);
								break;
							case "TipoDeEscrito":
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setTipo(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setTipo(valor);
								break;
							case "FechaHasta":
								Cargo.setFechaHasta(valor);
								break;
							case "FechaDesde":
								Cargo.setFechaDesde(valor);
								break;
							case "Expediente":
								Cargo.setExpediente(valor);
								break;
							case "Modo":
								Cargo.setModo(valor);
								break;
							case "Vinculo":
