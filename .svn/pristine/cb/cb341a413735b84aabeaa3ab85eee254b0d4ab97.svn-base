		public static void obtenerDatos(entidades nombreEntidad, String nroCasoPrueba) {
			
			XSSFSheet sheet = Ejecucion.eExcelDatos.getSheetAt(0);
			Iterator<Row> rowSolapaPruebasItera = sheet.iterator();
			Row rowSolapaPruebas = null;
			
			XSSFSheet sheetDatosCaso = null;
			Iterator<Row> rowDatosIterator = null;
			Row rowDatos = null;
			
			String atributo = null;
			String valor = null;
			
			
			while (rowSolapaPruebasItera.hasNext()) {
				rowSolapaPruebas= rowSolapaPruebasItera.next();
				if (rowSolapaPruebas.getRowNum() < 6) continue;
				
				if (rowSolapaPruebas.getCell(0).getStringCellValue().trim().equals(nroCasoPrueba.trim())) 
				{
					nombreCasoTestlink = 
							rowSolapaPruebas.getCell(0).getStringCellValue().trim() 
							+ " " +
							rowSolapaPruebas.getCell(3).getStringCellValue();
					sheetDatosCaso = Ejecucion.eExcelDatos.getSheet(rowSolapaPruebas.getCell(2).getStringCellValue().trim());
					rowDatosIterator = sheetDatosCaso.iterator();
					
					while (rowDatosIterator.hasNext()) {
						rowDatos = rowDatosIterator.next();
						if (rowDatos.getRowNum() < 5) continue;	
						
						if (rowDatos.getCell(0).getStringCellValue().trim().equals(nombreEntidad.toString())) {
							atributo = rowDatos.getCell(1).getStringCellValue();
							valor = rowDatos.getCell(2).getStringCellValue();
							
							switch (atributo) {
							case "PasarExpedientesA":
								Expediente.setPasarExpedientesA(valor);
								break;
							case "CUIJ":
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setCUIJ(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setCUIJ(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Actuaciones.toString().trim())) Actuaciones.setCUIJ(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Archivo.toString().trim())) Archivo.setCUIJ(valor);
								break;
							case "DatoDummy":
								PruebaDummy.setDatoDummy(valor);
								break;
							case "ApellidoYNombreActor":
								MEU.setApellidoActor(valor);
								break;
							case "Carácter":
								MEU.setCaracter(valor);
								break;
							case "Representante":
								MEU.setRepresentante(valor);
								break;
							case "Categoria":
								MEU.setCategoria(valor);
								break;
							case "MateriaPrincipal":
								if (nombreEntidad.toString().trim().equals(entidades.MEU.toString().trim())) MEU.setMateriaPrincipal(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setMateria(valor);
								break;
							case "FechaLibro":
								Cargo.setFechaLibro(valor); 
								break;
							case "HoraApertura":
								Cargo.setHoraApertura(valor);
								break;
							case "Hora":
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setHora(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setHora(valor);
								
								break;
							case "Comentarios":
								if (nombreEntidad.toString().trim().equals(entidades.Dummy.toString().trim())) PruebaDummy.setComentarios(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setComentarios(valor);
								break;
							case "HoraCierre":
								Cargo.setHoraCierre(valor);
								break;
							case "Presentante":
								if (nombreEntidad.toString().trim().equals(entidades.Expedientes.toString().trim())) Expediente.setPresentante(valor);
								if (nombreEntidad.toString().trim().equals(entidades.Cargo.toString().trim())) Cargo.setPresentante(valor);
								break;
							case "Solicita":
